    }
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@types/envinfo": "^7.8.1",
    "@types/react": "^18.0.15",
    "ava": "^4.3.1",
    "chalk": "^4.1.2",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "ink-testing-library": "^2.1.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "xo": "^0.39.1"
