  "license": "MIT",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "tsup src/index.tsx && chmod +x dist/index.js",
    "start": "npm run build && dist/index.js"
  },
  "files": [
    "dist/cli.js"
  ],
  "dependencies": {
    "axios": "^0.27.2",
    "envinfo": "^7.8.1",
    "fastify": "^4.2.1",
    "ink": "^3.2.0",
    "meow": "^9.0.0",
    "open": "^8.4.0",
    "pkce-challenge": "^3.0.0",
    "react": "^17.0.2"
  },
  "ava": {
    "typescript": {
      "extensions": [
        "tsx"
      ],
      "rewritePaths": {
        "source/": "dist/"
      }
    }
  },
  "xo": {
    "extends": "xo-react",
